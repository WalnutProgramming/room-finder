{
  "name": "room-finder",
  "version": "0.0.9",
  "repository": "github:WalnutProgramming/room-finder",
  "homepage": "https://walnutprogramming.github.io/room-finder/",
  "license": "AGPL-3.0-only",
  "main": "./dist/index.js",
  "module": "./dist/module.js",
  "types": "./dist/index.d.ts",
  "files": [
    "/dist",
    "/external-types"
  ],
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-numeric-separator": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-typescript": "^7.7.7",
    "@rollup/plugin-commonjs": "^11.0.0",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@types/jest": "^25.1.1",
    "babel-jest": "^25.1.0",
    "copyfiles": "^2.2.0",
    "embedme": "^1.19.0",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "rollup": "^1.29.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-terser": "^5.1.3",
    "typedoc": "^0.16.8",
    "typescript": "^3.7.3"
  },
  "dependencies": {
    "core-js": "3",
    "dijkstrajs": "^1.0.1"
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "yarn type-check -- --watch",
    "build": "yarn build:types && yarn bundle",
    "build-without-bundle": "yarn build:types && yarn build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "babel-compile": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "bundle": "rollup -c",
    "prep": "yarn build && yarn docs && yarn test",
    "prepublishOnly": "yarn prep",
    "readme": "yarn embedme README.md",
    "docs": "yarn readme && yarn typedoc --out docs --mode file --exclude **/*.test.ts src && copyfiles images/* docs",
    "test": "jest"
  },
  "prettier": {
    "trailingComma": "es5"
  }
}
